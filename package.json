{
  "name": "vue-json-miller-editor",
  "version": "0.1.11",
  "description": "A clean, minimal Vue 3 component for editing and validating JSON with Miller Columns interface",
  "type": "module",
  "main": "./dist/vue-json-edtr.umd.cjs",
  "module": "./dist/vue-json-edtr.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-json-edtr.js",
      "require": "./dist/vue-json-edtr.umd.cjs"
    },
    "./dist/vue-json-edtr.css": "./dist/vue-json-edtr.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.map",
    "README.md",
    "CONFIGURATION.md"
  ],
  "keywords": [
    "vue",
    "json",
    "editor",
    "validator",
    "schema",
    "typescript",
    "component"
  ],
  "author": "Redon Cela",
  "license": "MIT",
  "homepage": "https://github.com/reddyCe/vue-json-miller-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/reddyCe/vue-json-miller-editor.git"
  },
  "bugs": {
    "url": "https://github.com/reddyCe/vue-json-miller-editor/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p build-lib",
    "build-lib": "vite build --mode lib && vue-tsc --declaration --emitDeclarationOnly --skipLibCheck",
    "build-demo": "vite build --mode demo",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepublishOnly": "npm run build && npm test"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "terser": "^5.44.0",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.4"
  }
}
